########################################################
title=3sat.de
target=<param name="url" value="([^"]+)"
url=%s
ext_target=href *= *"([^r][^"]+)"
extension=wmv
quality=high
target=<param name="url" value="([^"]+)"
ext_target=href *= *"rtsp([^"]+)"
ext_build=http%s
extension=wmv
quality=high
########################################################
title=kino.de
target=flashvars.initItemXML *= *"([^"]+)
url=%s
ext_target=<url>([^<]+)</url> 
extension=flv
quality=high
########################################################
title=zdf.de
target="assetUrl": *"([^"]+)"
url=%s&bw=isdn&pp=wmp
ext_target=href *= *"([^"]+)"
info=Modem- oder ISDN-Verbindung
extension=asf
quality=low
target="assetUrl": *"([^"]+)"
url=%s&bw=dsl1000&pp=wmp
ext_target=href *= *"([^"]+)"
info=DSL- oder Breitband-Verbindung
extension=asf
quality=standard
target="assetUrl": *"([^"]+)"
url=%s&bw=dsl2000&pp=wmp
ext_target=href *= *"([^"]+)"
info=ab DSL-2000
extension=asf
quality=high
########################################################
title=br.de
target=player\.avaible\_url\['flashmedia'\]\['1'\] = "(http://[^"]+)";
url=%s
extension=flv
quality=standard
target=player\.avaible\_url\['flashmedia'\]\['2'\] = "(http://[^"]+)";
extension=flv
quality=high
target=player\.avaible\_url\['microsoftmedia'\]\['1'\] = "(http://[^"]+)";
extension=wmv
quality=standard
target=player\.avaible\_url\['microsoftmedia'\]\['2'\] = "(http://[^"]+)";
extension=wmv
quality=high
########################################################
title=tve.es
target=so.addVariable\("parseFile", "(/alacarta/player/\d+.xml)"\);
url=%s
build=http://www.rtve.es%s
ext_target=rtmp://stream.rtve.es/stream(/resources/alacarta/flv/.+?.flv)
ext_build=http://www.rtve.es%s
extension=flv
quality=standard
########################################################
title=cc2.de
target=swf\?file=(http://[^\&]+)
url=%s
info=Flash
extension=flv
quality=standard
target=<a href="([^"]+)">MP4 H.264
info=MP4 H.264
extension=mp4
quality=standard
target=<a href="([^"]+)">AVI Xvid
info=AVI Xvid
extension=avi
quality=high
